{
    "name": "php-http/throttle-plugin",
    "description": "Throttle/request limiter plugin for HTTPlug",
    "license": "MIT",
    "keywords": [
        "http",
        "httplug",
        "plugin",
        "ratelimit",
        "throttle"
    ],
    "authors": [
        {
            "name": "Gabriel Ostroluck√Ω",
            "email": "gabriel.ostrolucky@gmail.com"
        }
    ],
    "homepage": "https://github.com/php-http/throttle-plugin",
    "require": {
        "php": "^7.4 || ^8.0",
        "php-http/client-common": "^1.3 || ^2.0",
        "symfony/rate-limiter": "^5.2 || ^6.0"
    },
    "require-dev": {
        "nyholm/psr7": "^1.0",
        "php-http/mock-client": "^1.0",
        "ramsey/coding-standard": "^2.0",
        "phpcsstandards/phpcsextra": "^1.0.0-alpha3@alpha",
        "symfony/phpunit-bridge": "^6.0"
    },
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "Http\\Client\\Common\\Plugin\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\Http\\Client\\Common\\Plugin\\": "tests/"
        }
    },
    "config": {
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true,
            "ergebnis/composer-normalize": true
        }
    }
}
