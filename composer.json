{
    "name": "php-http/throttle-plugin",
    "description": "Throttle/request limiter plugin for HTTPlug",
    "license": "MIT",
    "keywords": [
        "http",
        "httplug",
        "plugin",
        "ratelimit",
        "throttle"
    ],
    "authors": [
        {
            "name": "Gabriel OstroluckÃ½",
            "email": "gabriel.ostrolucky@gmail.com"
        }
    ],
    "homepage": "https://github.com/php-http/throttle-plugin",
    "require": {
        "php": ">=7.4",
        "php-http/client-common": ">=1.3",
        "symfony/rate-limiter": ">=5.4"
    },
    "require-dev": {
        "nyholm/psr7": "^1.0",
        "php-http/mock-client": "^1.4",
        "ramsey/coding-standard": "^2.0",
        "symfony/phpunit-bridge": ">=6.0",
        "phpunit/phpunit": "^9.6",
        "psr/http-factory": "^1.1"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "Http\\Client\\Common\\Plugin\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\Http\\Client\\Common\\Plugin\\": "tests/"
        }
    },
    "config": {
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true,
            "ergebnis/composer-normalize": true,
            "php-http/discovery": true
        }
    }
}
